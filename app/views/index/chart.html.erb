<script type="text/javascript" charset="utf-8">
  $(function () {
          Highcharts.setOptions({
              global: {
                  timezoneOffset: -8 * 60
              }
          });
          
          $('#chart').highcharts({
              title: {
                  text: 'Progress of teams',
                  x: -20 //center
              },
              // subtitle: {
              //     text: 'Source: WorldClimate.com',
              //     x: -20
              // },
              xAxis: {
                  type: 'datetime',
                  dateTimeLabelFormats: {
                      day: '%H:%M'
                  },
                  title: {
                      text: 'Time'
                  },
                  min: Date.UTC(2014, 03, 17, 07, 00, 00),
                  max: Date.UTC(2014, 03, 17, 10, 00, 00),
                  // categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']
              },
              yAxis: {
                  title: {
                      text: 'Score'
                  },
                  min: 0,
                  max: 1000
                  // plotLines: [{
                  //     value: 0,
                  //     width: 1,
                  //     color: '#808080'
                  // }]
              },
              // tooltip: {
              //     valueSuffix: 'Â°C'
              // },
              legend: {
                  layout: 'vertical',
                  align: 'right',
                  verticalAlign: 'middle',
                  borderWidth: 0
              },
              series: [{
                  name: 'Tokyo',
                  data: [[<%= @time %>, 250], [Date.UTC(2014, 03, 17, 09, 40, 00), 500]]
              }]
          });
      });
</script>

<div id="chart"></div>